#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }

         int current=arr[0],highest=arr[0];
       for(int i=1;i<n;i++)
       {
           current=max(arr[i],current+arr[i]);
           highest=max(highest,current);
       }
       cout << highest << "\n";
        
}
