input : 5
        1 3 4 2 2
        
output : 2



#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int cnt=0;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(arr[i] == arr[j])
            {
                cnt= arr[i];
            }
        }
    }
    cout << cnt;

}
